# Environment Variables Template
# Copy this file to .env and fill in your actual API keys
# Command: cp .env.example .env
# DO NOT COMMIT THE .env FILE - It contains sensitive API keys

# Claude AI API
CLAUDE_API_KEY=your-claude-api-key-here

# Google Gemini API
GEMINI_API_KEY=your-gemini-api-key-here

# Letta AI
LETTA_API_KEY=your-letta-api-key-here
LETTA_PROJECT=your-letta-project-id
LETTA_AGENT_ID=your-letta-agent-id

# Vapi (Voice AI)
VAPI_PRIVATE_KEY=your-vapi-private-key
VAPI_PUBLIC_KEY=your-vapi-public-key
VAPI_ASSISTANT_ID=your-vapi-assistant-id

# Fish Labs API
FISH_LABS_API_KEY=your-fish-labs-api-key
FISH_LABS_MODEL_ID=your-fish-labs-model-id
FISH_LABS_MODEL_ID_2=your-fish-labs-model-id-2
FISH_LABS_MODEL_ID_3=your-fish-labs-model-id-3
FISH_LABS_MODEL_ID_4=your-fish-labs-model-id-4

# Vapi slacking off detection
VAPI_API_KEY=your-vapi-api-key
VAPI_PHONE_NUMBER_ID=your-vapi-phone-number-id
VAPI_SLACK_OFF_ASSISTANT_ID=your-vapi-slack-off-assistant-id

# Supabase configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
